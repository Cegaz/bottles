 <div class="filters mb-4 mt-4 d-flex justify-content-around">
    <div class="form-group mb-0">
        <div class="filter">
            <input type="text" class="search-box font-awesome" placeholder="&#xF002;">
        </div>
    </div>

    <div class="form-group mb-0">
        <div class="filter ml-1">
            <label class="checkbox-color mb-0 p-2 pointer" data-value='rouge'>
                <i class='fas fa-wine-bottle purple big-icon'></i>
            </label>
        </div>
        <div class="filter ml-1">
            <label class="checkbox-color mb-0 p-2 pointer" data-value='blanc'>
                <i class='fas fa-wine-bottle white big-icon'></i>
            </label>
        </div>
        <div class="filter ml-1">
            <label class="checkbox-color mb-0 p-2 pointer" data-value='rosé'>
                <i class='fas fa-wine-bottle pink big-icon'></i>
            </label>
        </div>
    </div>

    <div class="form-group d-flex">
        <img width="35px"
             src="data:image/svg+xml;utf8;base64,"
             class="mr-2"
        />
        <select class="form-control filter" name="area">
            <option value="">Région</option>
            {% for code, label in areas %}
                <option value="{{ code }}">{{ label }}</option>
            {% endfor %}
        </select>
    </div>

    <div class="form-group d-flex">
        <img width="35px"
             src="data:image/svg+xml;utf8;base64,"
             class="mr-2"
        />
        <select class="form-control filter" name="year">
            <option value="">Millésime</option>
            {% for year in years %}
                <option value="{{ year }}">{{ year }}</option>
            {% endfor %}
        </select>
    </div>

    {% if dluoYears is defined %}
        <div class="form-group d-flex">
        <img width="35px"
             src="data:image/svg+xml;utf8;base64,"
             class="mr-2"
        />
        <select class="form-control filter" name="dluo">
            <option value="">Date limite</option>
            {% for year in dluoYears %}
                <option value="{{ year }}">{{ year }}</option>
            {% endfor %}
        </select>
    </div>
{% endif %}
</div>
